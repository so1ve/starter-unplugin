{
	"name": "pkg-name",
	"version": "0.0.0",
	"packageManager": "pnpm@8.15.2",
	"author": "Ray <i@mk1.io> (@so1ve)",
	"description": "",
	"keywords": [
		"astro",
		"nuxt",
		"rollup",
		"rspack",
		"transform",
		"unplugin",
		"vite",
		"webpack"
	],
	"homepage": "https://github.com/so1ve/pkg-name#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/so1ve/pkg-name.git"
	},
	"bugs": {
		"url": "https://github.com/so1ve/pkg-name/issues"
	},
	"license": "MIT",
	"sideEffects": false,
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./*": "./*",
		"./astro": {
			"require": "./dist/astro.cjs",
			"import": "./dist/astro.mjs"
		},
		"./esbuild": {
			"require": "./dist/esbuild.cjs",
			"import": "./dist/esbuild.mjs"
		},
		"./nuxt": {
			"require": "./dist/nuxt.cjs",
			"import": "./dist/nuxt.mjs"
		},
		"./rollup": {
			"require": "./dist/rollup.cjs",
			"import": "./dist/rollup.mjs"
		},
		"./rspack": {
			"require": "./dist/rspack.cjs",
			"import": "./dist/rspack.mjs"
		},
		"./types": {
			"require": "./dist/types.cjs",
			"import": "./dist/types.mjs"
		},
		"./vite": {
			"require": "./dist/vite.cjs",
			"import": "./dist/vite.mjs"
		},
		"./webpack": {
			"require": "./dist/webpack.cjs",
			"import": "./dist/webpack.mjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "unbuild",
		"lint": "eslint . && prettier . --check",
		"lint:fix": "eslint . --fix && prettier . --write",
		"prepublishOnly": "nr build",
		"release": "bumpp --commit --push --tag && pnpm publish",
		"stub": "unbuild --stub",
		"test": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@nuxt/kit": "^3.8.0",
		"unplugin": "^1.5.0"
	},
	"devDependencies": {
		"@antfu/ni": "^0.21.8",
		"@nuxt/schema": "^3.8.0",
		"@so1ve/eslint-config": "^1.0.0-alpha.16",
		"@so1ve/prettier-config": "^1.0.0-alpha.16",
		"@types/node": "^20.8.10",
		"bumpp": "^9.2.0",
		"eslint": "^8.52.0",
		"prettier": "^3.0.3",
		"rollup": "^4.2.0",
		"typescript": "^5.2.2",
		"unbuild": "^2.0.0",
		"vite": "^4.5.0",
		"vitest": "^0.34.6",
		"webpack": "^5.89.0"
	},
	"peerDependencies": {
		"@nuxt/kit": "^3",
		"@nuxt/schema": "^3",
		"esbuild": "*",
		"rollup": "^3",
		"vite": ">=3",
		"webpack": "^4 || ^5"
	},
	"peerDependenciesMeta": {
		"webpack": {
			"optional": true
		},
		"rollup": {
			"optional": true
		},
		"vite": {
			"optional": true
		},
		"esbuild": {
			"optional": true
		},
		"@nuxt/kit": {
			"optional": true
		},
		"@nuxt/schema": {
			"optional": true
		}
	}
}
