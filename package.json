{
  "name": "pkg-name",
  "version": "0.0.0",
  "packageManager": "pnpm@7.29.3",
  "author": "Ray <i@mk1.io> (@so1ve)",
  "description": "",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "nuxt",
    "transform"
  ],
  "homepage": "https://github.com/so1ve/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/so1ve/pkg-name.git"
  },
  "bugs": {
    "url": "https://github.com/so1ve/pkg-name/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && tsup",
    "build:fix": "tsx scripts/postbuild.ts",
    "watch": "tsup --watch src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "unplugin": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.20.0",
    "@babel/types": "^7.21.2",
    "@so1ve/eslint-config": "^0.46.0",
    "@types/node": "^18.15.0",
    "bumpp": "^9.0.0",
    "eslint": "^8.31.0",
    "picocolors": "^1.0.0",
    "pnpm": "^7.29.1",
    "puild": "^1.3.2",
    "rimraf": "^4.4.0",
    "rollup": "^3.19.0",
    "tsup": "^6.6.3",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2",
    "webpack": "^5.76.0"
  }
}
